<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/header.ejs') %>

        <link rel="stylesheet" type="text/css" href="css/app.css" />
        <link rel="stylesheet" type="text/css" href="css/style.css" media="print" onload="this.media='all'"/>
</head>

<body>
    <div class="wrapper">
        <%- include('partials/sidebar.ejs', {active: active}) %>

            <div class="main">
                <%- include('partials/navbar.ejs') %>

                    <main class="content">
                        <div class="container-fluid">
                            <h1>Etsy Listing</h1>
                            <div class="d-flex mt-3">
                                <p>Product Type</p>
                                <select class="ml-2" id="dropdown-product-type">
                                    <option value="canvas">Canvas</option>
                                </select>
                            </div>
 
                            <div class="d-flex mt-3">
                                <p>Custom Images</p>
                                <input class='ml-2' type="text" id="custom-images">
                            </div>

                            <div class="d-flex mt-3">
                                <p>Main Images</p>
                                <input class='ml-2' type="text" id="main-images">
                            </div>

                            <div class="d-flex mt-3">
                                <p>Title</p>
                                <input class='ml-2' type="text" id="title">
                            </div>

                            <div class="d-flex mt-3">
                                <p>Tags</p>
                                <input class='ml-2' type="text" id="tags">
                            </div>

                            <div class="d-flex mt-3">
                                <p>Shop Name</p>
                                <input class='ml-2' type="text" id="shop-name">
                            </div>

                            <div class="d-flex mt-3">
                                <p>Product Description</p>
                                <input class='ml-2' type="text" id="description">
                            </div>

                            <div class="d-flex mt-3">
                                Personalization
                                <input class='ml-2' type="checkbox" id="personalization">
                            </div>

                            <button id="submit-btn" class="mt-3 button-primary">Submit</button>

                        </div>
                    </main>
            </div>
    </div>
</body>

<script type="text/javascript" src="js/extension/jquery.js"></script>
<script type="text/javascript" src="js/extension/socket.io.js"></script>
<script type="text/javascript" src="js/client_listing.js"></script>
<script type="text/javascript" src="js/extension/app.js"></script>

</html>