<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/header.ejs') %>

    <link rel="stylesheet" type="text/css" href="css/app.css?v=1.2" />
    <link rel="stylesheet" type="text/css" href="css/style.css?v=1.2" media="print" onload="this.media='all'"/>
    <link rel="stylesheet" type="text/css" href="css/toastr.min.css" media="print" onload="this.media='all'"/>
</head>

<body>
    <div class="wrapper">
        <%- include('partials/sidebar.ejs', {active: active}) %>

        <div class="main">
            <%- include('partials/navbar.ejs') %>

            <main class="content">
                <div class="container-fluid">
                    <h3>Dashboard</h3>

                    <div class="status-container mt-2">
                        <div>
                            <img src="img/server.png" alt="" height="100px">
                            <h5 id="process-status"></h5>
                        </div>
                        <div>
                            <img id="cc-img" src="img/cc.png" alt="" height="100px">
                            <div id="customcat-status"></div>
                        </div>
                        <div>
                            <img id="etsy-img" src="img/etsy.png" alt="" height="100px">
                            <div id="etsy-status"></div>
                        </div>
                    </div>

                    <button class="button-primary" id="ping-vps">Ping VPS</button>
                    <button class="ml-2 button-primary" id="ping-customcat">Ping Customcat</button>

                    <h4 class="mt-3">For developer only</h4>
                    <button class="button-primary" id='update-server'>Update Server</button>
                    <button class="ml-2 button-primary" id="run-add-tracking-my-btn">Run My's Tracking</button>
                    <button class="ml-2 button-primary" id="run-add-tracking-trang-btn">Run Trang's
                        Tracking</button>
                    <button class="ml-2 button-primary" id="check-limit-btn">Check limit</button>
                    <button class="ml-2 button-primary" id="open-all-vps-btn">Open VPS</button>

                    <h6 class="mt-3" id='last-updated'></h6>
                </div>
            </main>
        </div>
    </div>

    <script type="text/javascript" src="js/extension/jquery.js"></script>
    <script type="text/javascript" src="js/extension/socket.io.js"></script>
    <script type="text/javascript" src="js/extension/toastr.min.js"></script>
    <script type="text/javascript" src="js/extension/app.js"></script>
    <script type="text/javascript" src="js/index.js?v=1.1"></script>

</body>

</html>